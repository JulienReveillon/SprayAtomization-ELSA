FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.diesel 0
);

regions
(
    boxToCell
    {
        box (-550 0 -5) (0 120 5);
        fieldValues
        (
            volScalarFieldValue alpha.diesel 1
        );
    }
);


